<project name="local" default="help">
  <target name="help">
    <echo message="This component has no local build targets." />
  </target>
  <target name="phpdoc">
  	<exec command="phpdoc -d ${project.src.phpdir} -t ${project.review.docsdir}" logoutput="true"/>
  </target>
</project>
